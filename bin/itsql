#!/usr/bin/env bash
# set -ex
set -euo pipefail
IFS=$'\n\t'

intershop_host_path=$(pwd)
# home=$(realpath "$(readlink -f "$0" | xargs dirname)"/..)
home="$(realpath "$(realpath "${BASH_SOURCE[0]}" | xargs dirname)"/..)"
cd "$home"
source bin/_prepare-environment

#-----------------------------------------------------------------------------------------------------------
postgres_paged -f ./db/update-os-env.sql
postgres_paged "$@"




